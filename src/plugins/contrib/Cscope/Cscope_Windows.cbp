<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Cscope Windows" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(WX_VER)/share/CodeBlocks/plugins/Cscope" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(WX_VER)/plugins/contrib/Cscope" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-check-associations -ns -nd -p debug" />
				<Option host_application="../../../devel$(WX_VER)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add directory="../../../devel$(WX_VER)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel$(WX_VER)\share\CodeBlocks\Cscope.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CscopeConfig.cpp" />
		<Unit filename="CscopeConfig.h" />
		<Unit filename="CscopeConfigPanel.cpp" />
		<Unit filename="CscopeConfigPanel.h" />
		<Unit filename="CscopeEntryData.cpp" />
		<Unit filename="CscopeEntryData.h" />
		<Unit filename="CscopeParserThread.cpp" />
		<Unit filename="CscopeParserThread.h" />
		<Unit filename="CscopePlugin.cpp" />
		<Unit filename="CscopePlugin.h" />
		<Unit filename="CscopeProcess.cpp" />
		<Unit filename="CscopeProcess.h" />
		<Unit filename="CscopeStatusMessage.cpp" />
		<Unit filename="CscopeStatusMessage.h" />
		<Unit filename="CscopeTab.cpp" />
		<Unit filename="CscopeTab.h" />
		<Unit filename="CscopeView.cpp" />
		<Unit filename="CscopeView.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/CscopeConfigPanel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/CscopeConfigPanel.wxs" src="CscopeConfigPanel.cpp" hdr="CscopeConfigPanel.h" fwddecl="0" i18n="1" name="CscopeConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
