<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RegExTestbed Windows" />
		<Option pch_mode="2" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(WX_VER)/share/CodeBlocks/plugins/RegExTestbed" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(WX_VER)/plugins/contrib/regex_testbed" />
				<Option external_deps="../../../devel$(WX_VER)/libcodeblocks.a;" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel$(WX_VER)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_BUILD.CFLAGS)" />
				</Compiler>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel$(WX_VER)\share\codeblocks\RegExTestbed.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DcbDEBUG" />
			<Add option="-DSUPPORT_UTF8" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add directory="../../../devel$(WX_VER)" />
		</Linker>
		<Unit filename="RegExTestbed.cpp" />
		<Unit filename="RegExTestbed.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="regexdlg.cpp" />
		<Unit filename="regexdlg.h" />
		<Unit filename="regexdlg.xrc" />
		<Unit filename="wxsmith/RegExDlg.wxs" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/RegExDlg.wxs" src="regexdlg.cpp" hdr="regexdlg.h" xrc="regexdlg.xrc" fwddecl="0" i18n="1" name="RegExDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
