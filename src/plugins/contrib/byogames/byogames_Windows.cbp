<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Games Windows" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(WX_VER)/share/CodeBlocks/plugins/byogames.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs$(WX_VER)/plugins/contrib/byogames" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel$(WX_VER)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="-std=$(#CB_BUILD.CPP_STD)" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add directory="../../../devel$(WX_VER)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel$(WX_VER)\share\CodeBlocks\byogames.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="byocbtris.cpp" />
		<Unit filename="byocbtris.h" />
		<Unit filename="byoconf.cpp" />
		<Unit filename="byoconf.h" />
		<Unit filename="byogame.cpp" />
		<Unit filename="byogame.h" />
		<Unit filename="byogamebase.cpp" />
		<Unit filename="byogamebase.h" />
		<Unit filename="byogames.cpp" />
		<Unit filename="byogames.h" />
		<Unit filename="byogameselect.cpp" />
		<Unit filename="byogameselect.h" />
		<Unit filename="byosnake.cpp" />
		<Unit filename="byosnake.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/byoConf.wxs" />
		<Unit filename="wxsmith/byoGameSelect.wxs" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/byoGameSelect.wxs" src="byogameselect.cpp" hdr="byogameselect.h" fwddecl="0" i18n="1" name="byoGameSelect" language="CPP" />
					<wxPanel wxs="wxsmith/byoConf.wxs" src="byoconf.cpp" hdr="byoconf.h" fwddecl="0" i18n="1" name="byoConf" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
