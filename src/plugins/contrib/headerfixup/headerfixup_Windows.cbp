<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="HeaderFixup Windows" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(WX_VER)/share/CodeBlocks/plugins/headerfixup" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(WX_VER)/plugins/contrib/headerfixup" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel$(WX_VER)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DcbDEBUG" />
					<Add option="-Winvalid-pch" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="$(#WXWIDGETS.include)" />
					<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
					<Add directory="../../../devel$(WX_VER)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel$(WX_VER)\share\CodeBlocks\headerfixup.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
		</Linker>
		<Unit filename="bindings.cpp" />
		<Unit filename="bindings.h" />
		<Unit filename="configuration.cpp" />
		<Unit filename="configuration.h" />
		<Unit filename="defaults.cpp" />
		<Unit filename="execution.cpp" />
		<Unit filename="execution.h" />
		<Unit filename="fileanalysis.cpp" />
		<Unit filename="fileanalysis.h" />
		<Unit filename="headerfixup.cpp" />
		<Unit filename="headerfixup.h" />
		<Unit filename="helper.cpp" />
		<Unit filename="helper.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="protocol.cpp" />
		<Unit filename="protocol.h" />
		<Unit filename="todo.txt" />
		<Unit filename="wxsmith/configuration.wxs" />
		<Unit filename="wxsmith/execution.wxs" />
		<Unit filename="wxsmith/protocol.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxPanel wxs="wxsmith/configuration.wxs" src="configuration.cpp" hdr="configuration.h" fwddecl="1" i18n="1" name="Configuration" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/execution.wxs" src="execution.cpp" hdr="execution.h" fwddecl="1" i18n="1" name="Execution" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/protocol.wxs" src="protocol.cpp" hdr="protocol.h" fwddecl="1" i18n="1" name="Protocol" language="CPP" />
				</resources>
			</wxsmith>
			<SVNInside Login="" Password="" ConnectStatus="1" NeedsPassword="0" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
