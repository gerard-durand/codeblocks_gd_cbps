<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CppCheck/Vera++ Windows" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(WX_VER)/share/codeblocks/plugins/CppCheck.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs$(WX_VER)/plugins/contrib/CppCheck" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel$(WX_VER)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="0" />
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNOPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(#WXWIDGETS.INCLUDE)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(WX_VER)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel$(WX_VER)/share/codeblocks/CppCheck.zip resources/manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="ConfigPanel.cpp" />
		<Unit filename="ConfigPanel.h" />
		<Unit filename="CppCheck.cpp" />
		<Unit filename="CppCheck.h" />
		<Unit filename="CppCheckListLog.cpp" />
		<Unit filename="CppCheckListLog.h" />
		<Unit filename="wxsmith/ConfigPanel.wxs" />
		<Extensions>
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/ConfigPanel.wxs" src="ConfigPanel.cpp" hdr="ConfigPanel.h" fwddecl="1" i18n="1" name="ConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
