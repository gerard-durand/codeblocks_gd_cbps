<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Code::Blocks Share Config Windows" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../devel$(WX_VER)/cb_share_config" prefix_auto="0" extension_auto="1" />
				<Option working_dir="../../devel$(WX_VER)" />
				<Option object_output="../../.objs$(WX_VER)/tools/cb_share_config" />
				<Option type="0" />
				<Option compiler="gcc" />
			</Target>
			<Environment>
				<Variable name="WX_VER" value="$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DUSE_PCH" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="../../include" />
			<Add directory="../../include/tinyxml" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="txml" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add directory="../../devel$(WX_VER)" />
		</Linker>
		<Unit filename="app.cpp" />
		<Unit filename="app.h" />
		<Unit filename="mainframe.cpp" />
		<Unit filename="mainframe.h" />
		<Unit filename="wxsmith/MainFrame.wxs" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="app.cpp" main="MainFrame" init_handlers="necessary" language="CPP" />
				<resources>
					<wxFrame wxs="wxsmith/MainFrame.wxs" src="mainframe.cpp" hdr="mainframe.h" fwddecl="0" i18n="1" name="MainFrame" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
